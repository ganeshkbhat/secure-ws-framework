{
  "name": "secure-ws-framework",
  "version": "1.0.0",
  "description": "a simple easy to use express like web websocket framework with router. supports authentication",
  "main": "index.js",
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "unpkg": "index.min.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganeshkbhat/which-.git"
  },
  "bugs": {
    "url": "https://github.com/ganeshkbhat/which-/issues"
  },
  "homepage": "https://github.com/ganeshkbhat/which-#readme",
  "scripts": {
    "test": "mocha --reporter spec --recursive --timeout 60000",
    "uglify": "uglifyjs --compress --mangle reserved=['$','require','exports'] index.js -o index.min.js",
    "gitadd": "git add .",
    "build": "tsc"
  },
  "pre-commit": {
    "run": [
      "uglify",
      "gitadd",
      "test"
    ]
  },
  "keywords": [
    
  ],
  "author": "Ganesh <ganeshsurfs@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/eslint": "^8.56.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.16",
    "chai": "^4.4.1",
    "live-server": "^1.2.2",
    "mocha": "^10.2.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4",
    "unimported": "^1.31.1"
  }
}
